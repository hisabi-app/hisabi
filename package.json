        "format": "prettier --write resources/",
        "format:check": "prettier --check resources/",
        "lint": "eslint . --fix",
        "test": "jest",
        "types": "tsc --noEmit"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.24.3",
        "@babel/preset-env": "^7.24.3",
        "@babel/preset-react": "^7.16.7",
